<view class="weui-panel">
  <view class="weui-panel__bd my-info">
    <view class="weui-media-box weui-media-box_small-appmsg">
      <view class="weui-cells weui-cells_in-small-appmsg">
        <navigator url="./../edit/index" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
          <view class="weui-cell__hd">
            <image src="{{ avatarUrl }}" class="user-avator" alt="{{ nickName }}"/>
          </view>
          <view class="weui-cell__bd weui-cell_primary">
            <view>{{ nickName }}</view>
          </view>
          <view class="weui-cell__ft weui-cell__ft_in-access">编辑</view>
        </navigator>
      </view>
    </view>
  </view>
</view>

